{
  "name": "bos-ai",
  "version": "1.0.0",
  "description": "Business Operating System with AI Agent Suite for Solopreneur Excellence",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "webpack --mode production",
    "setup:core": "./deployment/scripts/setup-core.sh",
    "setup:full": "./deployment/scripts/setup-full.sh", 
    "setup:minimal": "./deployment/scripts/setup-minimal.sh",
    "coord": "node src/coordination/coord-cli.js",
    "mission": "node src/missions/mission-runner.js",
    "chassis:track": "node src/frameworks/business-chassis/tracker.js",
    "blueprint:create": "node src/frameworks/client-success-blueprint/creator.js",
    "agents:status": "node src/agents/status-monitor.js",
    "intelligence:report": "node src/intelligence/report-generator.js",
    "assets:generate": "node src/templates/asset-generator.js",
    "deploy": "npm run build && npm run setup:core",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docs:build": "jsdoc -c jsdoc.conf.json",
    "docs:serve": "serve docs/build"
  },
  "keywords": [
    "business-operating-system",
    "ai-agents", 
    "solopreneur",
    "business-chassis",
    "systematic-excellence",
    "automation",
    "entrepreneurship",
    "business-intelligence",
    "customer-success",
    "revenue-optimization"
  ],
  "author": "BOS-AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/BOS-AI.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/BOS-AI/issues"
  },
  "homepage": "https://github.com/yourusername/BOS-AI#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "uuid": "^9.0.0",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "mongoose": "^7.5.0",
    "redis": "^4.6.8",
    "node-cron": "^3.0.2",
    "openai": "^4.0.0",
    "@anthropic-ai/sdk": "^0.6.0",
    "mathjs": "^11.11.0",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "pdf-lib": "^1.17.1",
    "handlebars": "^4.7.8",
    "nodemailer": "^6.9.4",
    "compression": "^1.7.4",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "jsdoc": "^4.0.2",
    "serve": "^14.2.1",
    "@types/node": "^20.5.9"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": ["airbnb-base"],
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "consistent-return": "off"
    }
  }
}